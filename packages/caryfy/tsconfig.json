{
  "compilerOptions": {
    "jsx": "react-jsx", // or "react-jsx" for React 17+
    "module": "esnext",
    "target": "es5",
    "allowJs": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}